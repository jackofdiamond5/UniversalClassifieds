<Page
    x:Class="AutomaticListigs.Pages.Listings.SellListingsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AutomaticListigs.Pages.Listings"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="212,407,0,0" VerticalAlignment="Top" Click="cancelButton_Click"/>
        <Button x:Name="confirmButton" Content="Confirm" HorizontalAlignment="Left" Margin="60,407,0,0" VerticalAlignment="Top"/>
        <Button x:Name="updatePricesButton" Content="Update Prices" HorizontalAlignment="Left" Margin="60,347,0,0" VerticalAlignment="Top" Click="updatePricesButton_Click"/>
        <Button x:Name="updateUriButton" Content="Update Uri" HorizontalAlignment="Left" Margin="184,347,0,0" VerticalAlignment="Top" Click="updateUriButton_Click"/>

    </Grid>
</Page>
